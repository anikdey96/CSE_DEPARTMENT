@model IEnumerable<CSE_DEPARTMENT.Models.designationEm>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_HomeLayoutPage.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Designation)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Designation)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.did }) |
            @Html.ActionLink("Details", "Details", new { id=item.did }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.did })
        </td>
    </tr>
}

</table>
