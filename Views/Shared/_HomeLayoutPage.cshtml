
@{
    ViewBag.Title = "Home Page";
}
<head>
    <style>
        #div1 {
            font-size: 48px;
        }
    </style>

    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <!-- Custom fonts for this template-->
    <link href="~/Content/Admin Template/vendor/fontawesome-free/css/all.min.css" rel="stylesheet" />
    <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css2?family=Bangers&display=swap" rel="stylesheet">

    <!-- Custom styles for this template-->
    <link href="~/Content/Admin Template/css/sb-admin-2.min.css" rel="stylesheet" />
    <link href="~/Content/Admin Template/vendor/datatables/dataTables.bootstrap4.min.css" rel="stylesheet" />
    <link href="~/Content/Admin Template/css/about.css" rel="stylesheet" />
</head>




<body>

    <div id="wrapper">

        <!-- Sidebar -->
        <ul class="navbar-nav bg-gradient-primary sidebar sidebar-dark accordion" id="accordionSidebar">
            <a class="sidebar-brand d-flex align-items-center justify-content-center" href="#">
                <div class="d-flex justify-content-start">
                    <img src="~/Content/Admin Template/img/varsity-logo.png" />
                </div>



            </a>

            <!-- Divider -->
            <!-- Nav Item - Dashboard -->
            <li class="nav-item active">
                <a class="nav-link" href="#">
                    <i class='fas fa-desktop' style='font-size:24px'></i>
                    <span>CSE Department</span>
                </a>
            </li>


            <hr class="sidebar-divider">

            <!-- Heading -->
            <div class="sidebar-heading">
                <b>Student Section</b>
            </div>
            <li class="nav-item">
                <a class="nav-link collapsed" href="#" data-toggle="collapse" data-target="#collapseStudent" aria-expanded="true" aria-controls="collapseStudent">
                    <i class='fas fa-user-graduate' style='font-size:24px'></i>
                    <span>Student Information</span>
                </a>
                <div id="collapseStudent" class="collapse" aria-labelledby="headingStudent" data-parent="#accordionSidebar">
                    <div class="bg-white py-2 collapse-inner rounded">

                        @Html.ActionLink("Current Academic Status", "#", "#", null, new { @class = "collapse-item" })

                        @Html.ActionLink("Previous Academic Status", "#", "#", null, new { @class = "collapse-item" })

                        @Html.ActionLink("Co-Curricular Activities", "#", "#", null, new { @class = "collapse-item" })
                        @Html.ActionLink("Result", "#", "#", null, new { @class = "collapse-item" })
                    </div>

                </div>
            </li>

            <hr class="sidebar-divider">

            <!-- Heading -->
            <div class="sidebar-heading">
                Teacher Section
            </div>
            <li class="nav-item">
                <a class="nav-link collapsed" href="#" data-toggle="collapse" data-target="#collapseEmployee" aria-expanded="true" aria-controls="collapseEmployee">
                    <i class='fas fa-user-tie' style='font-size:24px'></i>
                    <span>Teacher Information</span>
                </a>
                <div id="collapseEmployee" class="collapse" aria-labelledby="headingEmployee" data-parent="#accordionSidebar">
                    <div class="bg-white py-2 collapse-inner rounded">


                        @Html.ActionLink("Profile", "#", "#", null, new { @class = "collapse-item" })
                        @Html.ActionLink("Career", "#", "#", null, new { @class = "collapse-item" })

                    </div>
                </div>
            </li>


            <div class="sidebar-heading">
                <b>Staff Section</b>
            </div>
            <li class="nav-item">
                <a class="nav-link collapsed" href="#" data-toggle="collapse" data-target="#collapseUser" aria-expanded="true" aria-controls="collapseUser">
                    <i class='fas fa-user-alt' style='font-size:24px'></i>
                    <span>Staff Information</span>
                </a>
                <div id="collapseUser" class="collapse" aria-labelledby="headingUser" data-parent="#accordionSidebar">
                    <div class="bg-white py-2 collapse-inner rounded">

                        @Html.ActionLink("Profile", "Index", "usertypes", null, new { @class = "collapse-item" })

                        @Html.ActionLink("Career", "Index", "users", null, new { @class = "collapse-item" })

                    </div>

                </div>
            </li>

            <!-- Divider -->
            <hr class="sidebar-divider">

            <!-- Heading -->
            <div class="sidebar-heading">
                Academic Section

            </div>


            <!-- Nav Item - Pages Collapse Menu -->
            <li class="nav-item">
                <a class="nav-link collapsed" href="#" data-toggle="collapse" data-target="#collapseTwo" aria-expanded="true" aria-controls="collapseTwo">
                    <i class='fas fa-book-open' style='font-size:24px'></i>
                    <span>E-Library</span>
                </a>

            </li>

            <!-- Nav Item - Utilities Collapse Menu -->
            <li class="nav-item">
                <a class="nav-link collapsed" href="#" data-toggle="collapse" data-target="#collapseUtilities" aria-expanded="true" aria-controls="collapseUtilities">
                    <i class='fas fa-laptop-code' style='font-size:24px'></i>
                    <span>Class Materials</span>
                </a>

            </li>

            <!-- Nav Item - Sessions Collapse Menu -->
            <li class="nav-item">
                <a class="nav-link collapsed" href="#" data-toggle="collapse" data-target="#collapseSessions" aria-expanded="true" aria-controls="collapseSessions">
                    <i class='fas fa-chalkboard' style='font-size:24px'></i>
                    <span>E-Notice</span>
                </a>

            </li>

            <li class="nav-item">
                <a class="nav-link collapsed" href="#" data-toggle="collapse" data-target="#collapseClass" aria-expanded="true" aria-controls="collapseClass">
                    <i class='far fa-calendar-alt' style='font-size:24px'></i>
                    <span>Class Routine</span>
                </a>

            </li>
            <li class="nav-item">
                <a class="nav-link collapsed" href="#" data-toggle="collapse" data-target="#collapseClass" aria-expanded="true" aria-controls="collapseClass">
                    <i class='fas fa-chalkboard-teacher' style='font-size:24px'></i>
                    <span>Google Class Room</span>
                </a>

            </li>
            <li class="nav-item">
                <a class="nav-link collapsed" href="#" data-toggle="collapse" data-target="#collapseClass" aria-expanded="true" aria-controls="collapseClass">
                    <i class='fab fa-facebook-messenger' style='font-size:24px'></i>
                    <span>Messaging</span>
                </a>

            </li>


            <!-- Divider -->
            <hr class="sidebar-divider d-none d-md-block">

            <!-- Sidebar Toggler (Sidebar) -->
            <div class="text-center d-none d-md-inline">
                <button class="rounded-circle border-0" id="sidebarToggle"></button>
            </div>






        </ul>
        <!-- End of Sidebar -->
        <!-- Content Wrapper -->
        <div id="content-wrapper" class="d-flex flex-column">

            <!-- Main Content -->

            <div id="content">

                <!-- Topbar -->
                <nav class="navbar navbar-expand navbar-light bg-white topbar mb-4 static-top shadow">

                    <!-- Sidebar Toggle (Topbar) -->
                    <button id="sidebarToggleTop" class="btn btn-link d-md-none rounded-circle mr-3">
                        <i class="fa fa-bars"></i>
                    </button>

                    <!-- Topbar Search -->
                    <!-- Topbar Navbar -->
                    <ul class="navbar-nav ml-auto">

                        <!-- Nav Item - Search Dropdown (Visible Only XS) -->
                        <li class="nav-item dropdown no-arrow d-sm-none">
                            <a class="nav-link dropdown-toggle" href="#" id="searchDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                <i class="fas fa-search fa-fw"></i>
                            </a>

                            <div class="topbar-divider d-none d-sm-block"></div>



                    </ul>

                </nav>

                <!-- End of Topbar -->
                <!-- Begin Page Content -->
                <div class="container-fluid">
                        @RenderBody()
                    </div>
                <!-- /.container-fluid -->

            </div>


        </div>

    </div>


    <!-- End of Content Wrapper -->
    <!-- End of Page Wrapper -->
    <!-- Scroll to Top Button-->
    <a class="scroll-to-top rounded" href="#page-top">
        <i class="fas fa-angle-up"></i>
    </a>




    <!-- Bootstrap core JavaScript-->

    <script src="~/Content/Admin Template/vendor/jquery/jquery.min.js"></script>
    <script src="~/Content/Admin Template/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

    <!-- Core plugin JavaScript-->
    <script src="~/Content/Admin Template/vendor/jquery-easing/jquery.easing.min.js"></script>

    <!-- Custom scripts for all pages-->

    <script src="~/Content/Admin Template/js/sb-admin-2.min.js"></script>

    <!-- Page level plugins -->
    <script src="~/Content/Admin Template/vendor/chart.js/Chart.min.js"></script>

    <!-- Page level custom scripts -->
    <script src="~/Content/Admin Template/js/demo/chart-area-demo.js"></script>
    <script src="~/Content/Admin Template/js/demo/chart-pie-demo.js"></script>


    <!-- Page level plugins -->
    <script src="~/Content/Admin Template/vendor/datatables/jquery.dataTables.min.js"></script>
    <script src="~/Content/Admin Template/vendor/datatables/dataTables.bootstrap4.min.js"></script>

    <!-- Page level custom scripts -->
    <script src="~/Content/Admin Template/js/demo/datatables-demo.js"></script>


    <!-- Bootstrap core JavaScript-->
    <script src="~/Content/Admin Template/vendor/jquery/jquery.min.js"></script>
    <script src="~/Content/Admin Template/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

    <!-- Core plugin JavaScript-->
    <script src="~/Content/Admin Template/vendor/jquery-easing/jquery.easing.min.js"></script>

    <!-- Custom scripts for all pages-->
    <script src="~/Content/Admin Template/js/sb-admin-2.min.js"></script>

    <!-- Page level plugins -->
    <script src="~/Content/Admin Template/vendor/chart.js/Chart.min.js"></script>

    <!-- Page level custom scripts -->
    <script src="~/Content/Admin Template/js/demo/chart-area-demo.js"></script>
    <script src="~/Content/Admin Template/js/demo/chart-pie-demo.js"></script>


    <!-- Page level plugins -->
    <script src="~/Content/Admin Template/vendor/datatables/jquery.dataTables.min.js"></script>
    <script src="~/Content/Admin Template/vendor/datatables/dataTables.bootstrap4.min.js"></script>

    <!-- Page level custom scripts -->
    <script src="~/Content/Admin Template/js/demo/datatables-demo.js"></script>



</body>

